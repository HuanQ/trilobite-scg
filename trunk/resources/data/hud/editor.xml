				<Rectangle x="0" y="0" z="8" sizex="0.375" sizey="0.15">
					<Texture>rect2x1.gif</Texture>
				</Rectangle>
				<Text x="0" y="0" z="9" txt="Move" size="2">
					<Color r="1" g="0" b="0"/>
				</Text>
			</Shape>
		</Button>
