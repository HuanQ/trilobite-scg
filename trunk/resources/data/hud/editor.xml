<Trilobite>
	<Background zone="left">
		<Image>
			<Point x="0.5" y="0.5"/>
			<Shape>
				<Rectangle x="0" y="0" z="6" sizex="1" sizey="1" stretch="true">
					<Texture>rect1x2.gif</Texture>
				</Rectangle>
			</Shape>
		</Image>
	</Background>
	
	<Tools zone="left">
		<Button>
			<Point x="0.2875" y="0.05"/>
			<Function>ADDCIRCLE</Function>
			<Shape>
				<Rectangle x="0" y="0" z="8" sizex="0.375" sizey="0.15">
					<Texture>rect2x1.gif</Texture>
				</Rectangle>
				<Text x="0" y="0" z="9" txt="Circle" size="2">
					<Color r="1" g="0" b="0"/>
				</Text>
			</Shape>
		</Button>
		
		<Button>
			<Point x="0.7625" y="0.05"/>
			<Function>ADDSQUARE</Function>
			<Shape>
				<Rectangle x="0" y="0" z="8" sizex="0.375" sizey="0.15">
					<Texture>rect2x1.gif</Texture>
				</Rectangle>
				<Text x="0" y="0" z="9" txt="Square" size="2">
					<Color r="1" g="0" b="0"/>
